#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int>v;
    for(int i=0; i<n; i++)
        {
            int x;
            cin>>x;
            v.push_back(x);

        }
         int maxsum=v[0];
        int currentSum=v[0];
        for(int i=1; i<v.size(); i++)
        {
            currentSum=max(currentSum + v[i],v[i]);
            maxsum=max(maxsum,currentSum);
        }


        cout<<maxsum<<endl;
    }
